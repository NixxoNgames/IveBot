    "mongodb": "^4.0.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^16.4.7",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
