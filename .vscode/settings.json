{
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
    "eslint.options": {
      "extensions": [
        ".js",
        ".ts",
        ".jsx",
        ".tsx"
      ]
    },
    "eslint.enable": true,
    "eslint.autoFixOnSave": true,
    "eslint.packageManager": "yarn",
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "typescript.preferences.importModuleSpecifierEnding": "js"
  }